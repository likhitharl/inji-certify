{
	"vcTemplate": {
  "nameSpaces": {
    {{#each nameSpaces}}
    "{{namespace}}": [
      {{#each elements}}
      {
        "digestID": {{digestID}},
        "elementIdentifier": "{{elementIdentifier}}",
        "elementValue": {{#if isString}}"{{elementValue}}"{{else}}"RAW:{{elementValue}}"{{/if}}
      }{{#unless @last}},{{/unless}}
      {{/each}}
    ]{{#unless @last}},{{/unless}}
    {{/each}}
  },
  "docType": "${_docType}",
  "validityInfo": {
    "validFrom": "${_validFrom}",
    "validUntil": "${_validUntil}"
  }
},
    "contextURLs": [
	      {{#each contextURLs}}
	            "{{contextURLs}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
	"credentialTypes": [
	      {{#each credentialTypes}}
	            "{{credentialTypes}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],       	
    "credentialFormat": "{{credentialFormat}}",
    "credentialConfigKeyId": "{{credentialConfigKeyId}}",    
    "didUrl": "{{didUrl}}",        
    "keyManagerAppId" : "{{keyManagerAppId}}",
    "keyManagerRefId" : "{{keyManagerRefId}}",
    "signatureAlgo" : "{{signatureAlgo}}",
    "signatureCryptoSuite" : "{{signatureCryptoSuite}}",    
    "sdClaim" : "{{sdClaim}}",
    "doctype" : "{{doctype}}",    
    "displayOrder" : [
	      {{#each displayOrder}}
	            "{{displayOrder_values}}"
	          {{#unless @last}},{{/unless}}
	      	{{/each}}
        ],
    "scope" : "{{scope}}",
    "metaDataDisplay" : [{
    "name": "{{display_name}}",
    "locale": "en",
    "logo": {
      "url": "https://mosip.github.io/inji-config/logos/agro-vertias-logo.png",
      "alt_text": "land statement Logo"
    },
    "background_color": "#ebfaff",
    "background_image": {"uri": "https://mosip.github.io/inji-config/logos/agro-vertias-logo.png"},
    "text_color": "#000000"
  }]
}